<%@ page contentType="text/html; charset=utf-8"%>
<html>
    <head>
        <meta http-equiv="pragma" content="no-cache"/>
        <meta http-equiv="cache-control" content="no-cache,must-revalidate"/>
        <meta content="width=device-width,user-scalable=no" name="viewport"/>
        <script type="text/javascript" src="./loginMode2/js/login.js"></script>
    </head>
    <body>
        <div style="width:100%;height:30%">
            <img style="width: 100%;height:100%" src="./loginMode2/QC/picture/jduLOGO.png"/>
        </div>
        <form action="./loginMobile.mobile?reqCode=login" method="post">
	        <table align="center">
	            <tr>
	                <td align="right">用户名:</td>
	                <td><input type="text" name="username"  id="username"/></td>
	            </tr>
	            <tr>
	                <td align="right">密码:</td>
	                <td><input type="password" name="password" id="password"/></td>
	            </tr>
	            <tr>
	               <td colspan="2">
	                   <span style="color:red">记录最后一次登录的账号</span>
	               </td>
	                <td align="right" style="display:none"><input type="checkbox" id="isRememberAccount" checked="checked" name="rmbAccount"/>记住帐号</td>
	                <!-- 
	                <td style="display:none"><input type="checkbox" id="isRememberPwd"/>记住密码</td>
	                 -->
	            </tr>
	            <tr>
	                <td>
	                    <input type="submit" value="登录" id="loginButton"/>
	                </td>
	                <td style="display:none">
	                    <input type="button" value="使用说明"/>
	                </td>
	            </tr>
	        </table>
        </form>
    </body>
</html>
